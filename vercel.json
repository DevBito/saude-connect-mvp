{
  "version": 2,
  "builds": [
    {
      "src": "api/backend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/backend"
    },
    {
      "src": "/health",
      "dest": "/api/backend"
    },
    {
      "src": "/",
      "dest": "/api/backend"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
